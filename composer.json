{
    "name": "monkfish-fr/monkfish_slick",
    "description": "Install and enable Slick module (and sub-modules).",
    "type": "drupal-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Fabien TAVERNIER",
            "email": "contact@monkfish.fr",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/1.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/dinbror/blazy.git",
                    "type": "git",
                    "reference": "1.8.2"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "dinbror/blazy": "1.8.2",
        "kenwheeler/slick": "1.8.0",
        "drupal/slick_extras": "^1.0"
    }
}
